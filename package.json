{
  "name": "@orioro/biblioteca-data-api",
  "version": "0.0.0-semantic-release",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/orioro/node-biblioteca-data-api",
  "bugs": "https://github.com/orioro/node-biblioteca-data-api/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/orioro/node-biblioteca-data-api.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "test": "jest --runInBand --testTimeout=60000 --collectCoverage",
    "readme": "readme",
    "update-dependencies": "npx npm-check-updates -u",
    "tsc:declaration": "tsc --declaration --emitDeclarationOnly --declarationDir dist",
    "tsc:validate": "tsc --noEmit",
    "commit": "cz",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier src --check",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "prettier": "prettier src",
    "ci-check": "yarn test && yarn tsc:validate && yarn lint",
    "dev": "keystone-next dev",
    "start": "keystone-next start",
    "build": "keystone-next build",
    "postinstall": "keystone-next postinstall"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@orioro/jest-util": "^1.7.0",
    "@orioro/readme": "^1.0.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-jest": "^27.0.6",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rollup": "^2.56.2",
    "semantic-release": "^17.4.5",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "dependencies": {
    "@keystone-next/auth": "^31.0.0",
    "@keystone-next/fields": "^14.0.0",
    "@keystone-next/keystone": "^24.0.1",
    "@orioro/parse-env": "^1.0.1",
    "dotenv": "^10.0.0",
    "ms": "^2.1.3"
  }
}
